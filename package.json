{
  "name": "use-rhf-should-unregister",
  "version": "1.0.0",
  "description": "shouldUnregister polyfill for React Hook Form v7",
  "author": "iamacook",
  "license": "MIT",
  "repository": "iamacook/use-rhf-should-unregister",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-hook-form": "^7.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/runtime": "^7.13.10",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@rollup/plugin-url": "^6.0.0",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.45.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
}
